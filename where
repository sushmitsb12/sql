SELECT ID, NAME, SALARY 
FROM CUSTOMERS
WHERE SALARY > 2000;


SELECT ID, NAME, SALARY 
FROM CUSTOMERS
WHERE NAME = 'Hardik';


SELECT *
FROM Customer
WHERE City IN ('Paris','London')
 
 
SELECT Id, ProductName, UnitPrice
  FROM Product
 WHERE UnitPrice NOT IN (10,20,30,40,50)
 
 
 
 SELECT * FROM Student
 WHERE ROLL_NO BETWEEN 1 AND 3;
 
 
 SELECT * FROM Student
 WHERE NAME LIKE 'S%';
