 CREATE TABLE CUSTOMERS(
 ID   INT              NOT NULL,
 NAME VARCHAR (20)     NOT NULL,
 AGE  INT              NOT NULL,
 ADDRESS  CHAR (25) ,
 SALARY   DECIMAL (18, 2),       
 PRIMARY KEY (ID)
 
 
 
 CREATE TABLE Persons (
 PersonID int,
 LastName varchar(255),
 FirstName varchar(255),
 Address varchar(255),
 City varchar(255)
 
 
CREATE TABLE TestTable AS
SELECT customername, contactname
FROM customers



CREATE TABLE Employee  
(  
EmployeeID int,  
FirstName varchar(255),  
LastName varchar(255),  
Email varchar(255),  
AddressLine varchar(255),  
City varchar(255)  
);



SQL> CREATE TABLE STUDENTS (  
ID INT NOT NULL,  
NAME VARCHAR (20) NOT NULL,  
AGE INT NOT NULL,  
ADDRESS CHAR (25),  
PRIMARY KEY (ID)  
);  
